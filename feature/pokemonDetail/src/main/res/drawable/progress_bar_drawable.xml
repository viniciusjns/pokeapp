<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ResourceName">
    <item android:id="@+id/progressBarRoundedBg">
        <shape>
            <corners android:radius="@dimen/dimen_8dp" />
            <solid android:color="@color/black" />
        </shape>
    </item>

    <item
        android:id="@+id/progressBarRoundedProgress"
        android:bottom="@dimen/dimen_8dp"
        android:left="@dimen/dimen_8dp"
        android:right="@dimen/dimen_8dp"
        android:top="@dimen/dimen_8dp">

        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="@dimen/dimen_8dp" />
                <solid android:color="@color/black" />
            </shape>
        </scale>
    </item>
</layer-list>